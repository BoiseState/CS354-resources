:vpi_module "/usr/local/lib/ivl/v2009.vpi";
S_0xc78d00 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0xc786f0 .scope module, "sto" "sto" 3 3;
 .timescale -9 -10;
P_0xc78880 .param/l "BITS" 0 3 3, +C4<00000000000000000000000000001000>;
v0xcba380_0 .var "clk", 0 0;
v0xcba440_0 .var/i "i", 31 0;
v0xcba500_0 .var "ie", 0 0;
v0xcba600_0 .var "init", 0 0;
v0xcba6d0_0 .var "inp", 0 7;
v0xcba7c0_0 .net "oe", 0 0, v0xcba010_0;  1 drivers
v0xcba890_0 .net "out", 0 7, v0xcba120_0;  1 drivers
E_0xca4b90 .event anyedge, v0xcba010_0;
S_0xc78920 .scope module, "s" "Store" 3 11, 4 1 0, S_0xc786f0;
 .timescale -9 -10;
    .port_info 0 /INPUT 1 "CLK";
    .port_info 1 /INPUT 8 "INP";
    .port_info 2 /OUTPUT 8 "OUT";
    .port_info 3 /INPUT 1 "IE";
    .port_info 4 /OUTPUT 1 "OE";
    .port_info 5 /INPUT 1 "INIT";
P_0xc8f060 .param/l "BITS" 0 4 1, +C4<00000000000000000000000000001000>;
P_0xc8f0a0 .param/l "FIRST" 0 4 1, +C4<00000000000000000000000000000000>;
v0xc91940_0 .net "CLK", 0 0, v0xcba380_0;  1 drivers
v0xc92700_0 .net "IE", 0 0, v0xcba500_0;  1 drivers
v0xcb9e90_0 .net "INIT", 0 0, v0xcba600_0;  1 drivers
v0xcb9f30_0 .net "INP", 7 0, v0xcba6d0_0;  1 drivers
v0xcba010_0 .var "OE", 0 0;
v0xcba120_0 .var "OUT", 7 0;
v0xcba200_0 .var "ie", 0 0;
E_0xca50e0 .event posedge, v0xc91940_0;
    .scope S_0xc78920;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0xcba010_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0xcba200_0, 0;
    %end;
    .thread T_0;
    .scope S_0xc78920;
T_1 ;
    %wait E_0xca50e0;
    %load/vec4 v0xc92700_0;
    %flag_set/vec4 9;
    %flag_get/vec4 9;
    %jmp/0 T_1.2, 9;
    %load/vec4 v0xcba200_0;
    %nor/r;
    %and;
T_1.2;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %load/vec4 v0xcb9e90_0;
    %flag_set/vec4 8;
    %jmp/0 T_1.3, 8;
    %pushi/vec4 0, 0, 32;
    %jmp/1 T_1.4, 8;
T_1.3 ; End of true expr.
    %load/vec4 v0xcb9f30_0;
    %pad/u 32;
    %jmp/0 T_1.4, 8;
 ; End of false expr.
    %blend;
T_1.4;
    %pad/u 8;
    %assign/vec4 v0xcba120_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0xcba010_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0xcba200_0, 0;
T_1.0 ;
    %load/vec4 v0xc92700_0;
    %nor/r;
    %flag_set/vec4 9;
    %flag_get/vec4 9;
    %jmp/0 T_1.7, 9;
    %load/vec4 v0xcba200_0;
    %and;
T_1.7;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.5, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0xcba010_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0xcba200_0, 0;
T_1.5 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0xc786f0;
T_2 ;
    %delay 75, 0;
    %load/vec4 v0xcba380_0;
    %inv;
    %store/vec4 v0xcba380_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0xc786f0;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0xcba380_0, 0, 1;
    %delay 300, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0xcba500_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0xcba600_0, 0, 1;
    %delay 300, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0xcba500_0, 0, 1;
T_3.0 ;
    %load/vec4 v0xcba7c0_0;
    %cmpi/ne 1, 0, 1;
    %jmp/0xz T_3.1, 6;
    %wait E_0xca4b90;
    %jmp T_3.0;
T_3.1 ;
    %delay 300, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0xcba500_0, 0, 1;
T_3.2 ;
    %load/vec4 v0xcba7c0_0;
    %nor/r;
    %cmpi/ne 1, 0, 1;
    %jmp/0xz T_3.3, 6;
    %wait E_0xca4b90;
    %jmp T_3.2;
T_3.3 ;
    %pushi/vec4 1, 0, 32;
    %store/vec4 v0xcba440_0, 0, 32;
T_3.4 ;
    %load/vec4 v0xcba440_0;
    %cmpi/s 10, 0, 32;
    %jmp/0xz T_3.5, 5;
    %delay 300, 0;
    %load/vec4 v0xcba440_0;
    %pad/s 8;
    %store/vec4 v0xcba6d0_0, 0, 8;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0xcba600_0, 0, 1;
    %vpi_call/w 3 28 "$display", "sto(0..9)=%1d,%1d", v0xcba890_0, v0xcba6d0_0 {0 0 0};
    %delay 300, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0xcba500_0, 0, 1;
T_3.6 ;
    %load/vec4 v0xcba7c0_0;
    %cmpi/ne 1, 0, 1;
    %jmp/0xz T_3.7, 6;
    %wait E_0xca4b90;
    %jmp T_3.6;
T_3.7 ;
    %delay 300, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0xcba500_0, 0, 1;
T_3.8 ;
    %load/vec4 v0xcba7c0_0;
    %nor/r;
    %cmpi/ne 1, 0, 1;
    %jmp/0xz T_3.9, 6;
    %wait E_0xca4b90;
    %jmp T_3.8;
T_3.9 ;
    ; show_stmt_assign_vector: Get l-value for compressed += operand
    %load/vec4 v0xcba440_0;
    %pushi/vec4 1, 0, 32;
    %add;
    %store/vec4 v0xcba440_0, 0, 32;
    %jmp T_3.4;
T_3.5 ;
