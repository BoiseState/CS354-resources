title = {{Mathematical Foundations for a Compositional Distributional Model of Meaning}},
url = {http://www.cl.cam.ac.uk/{~}sc609/pubs/lambek11.pdf{\%}5Cnhttp://arxiv.org/abs/1003.4394},
