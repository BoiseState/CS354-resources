isbn = {0010-0277},
issn = {00100277},
journal = {Cognition},
keywords = {Depicted events,Eye-movements,Incremental thematic role disambiguation,Language comprehension,Ontological categories,Reference,psycholinguistics},
mendeley-tags = {psycholinguistics},
number = {1},
pages = {95--127},
pmid = {15629475},
publisher = {MIT Press},
title = {{The influence of the immediate visual context on incremental thematic role-assignment: Evidence from eye-movements in depicted events}},
