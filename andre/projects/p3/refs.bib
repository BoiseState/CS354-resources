isbn = {0018-9219 VO  - 88},
issn = {00189219},
keywords = {Acoustic modeling,Acoustic-phonetics,Articulation,Automatic recognition and understanding,Bayes risk,Cepsiral distance,Continuous speech recognition,Detection-based approach,Dialogue systems,Discriminative training,Dynamic programming,nlu},
