@article{Montague1970a,
abstract = {Introduction: While sentence comprehension has been reported to be defective in frontotemporal dementia (FTD), it is still unclear if this disorder reflects the presence of syntactic impairment, or may be attributed to other factors, such as executive or working memory dysfunction. In order to assess the status of syntactic knowledge in a group of patients belonging to the FTD spectrum, we investigated their ability to detect violations of Universal Grammar principles in a sentence judgement task. Methods: The group included four semantic dementia patients (SD), nine frontal variant of FTD patients (FvFTD), 15 progressive supranuclear palsy (PSP) patients, and 11 corticobasal degeneration syndrome (CBDS) patients. Their performance was compared to a group of 10 patients with mild probable Alzheimer disease (AD) and to 10 healthy volunteers. The patients underwent a standard aphasia test and a sentence comprehension test. The experimental study included five kinds of violations: semantic coherence (SC), verb-subject agreement (VSAgr), pronominalization involving clitic movement (ClM), interrogatives (WhS) and contrastive focus constructions (CFC). Results: The FTD patients performed within normal range in the aphasia test, and in the sentence comprehension test. Within the FTD subgroups, only patients with CBDS were significantly impaired in detecting three of the five kinds of violations. AD patients were also impaired in the detection of WhS and SC anomalies and in sentence comprehension. Discussion: The present findings indicate that, within the FTD spectrum, an impairment of syntactic knowledge can be found only in CBDS patients, even in the absence of clinical evidence of aphasia. {\textcopyright} 2007 Elsevier Ltd. All rights reserved.},
author = {Cotelli, Maria and Borroni, Barbara and Manenti, Rosa and Ginex, Valeria and Calabria, Marco and Moro, Andrea and Alberici, Antonella and Zanetti, Marina and Zanetti, Orazio and Cappa, Stefano F. and Padovani, Alessandro},
doi = {10.1016/j.neuropsychologia.2007.05.012},
isbn = {0028-3932 (Print)$\backslash$r0028-3932 (Linking)},
issn = {00283932},
journal = {Neuropsychologia},
keywords = {CBDS,Comprehension,FTD,Language impairment,PSP,Syntax},
number = {13},
pages = {3015--3023},
pmid = {17640688},
title = {{Universal grammar in the frontotemporal dementia spectrum. Evidence of a selective disorder in the corticobasal degeneration syndrome}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18561548},
volume = {45},
year = {2007}
}
