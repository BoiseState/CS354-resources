@article{Kruijff2007b,
abstract = {The paper presents an HRI architecture for human-augmented mapping,$\backslash$nwhich has been implemented and tested on an autonomous mobile robotic$\backslash$nplatform. Through interaction with a human, the robot can augment$\backslash$nits autonomously acquired metric map with qualitative information$\backslash$nabout locations and objects in the environment. The system implements$\backslash$nvarious interaction strategies observed in independently performed$\backslash$nWizard-of-Oz studies. The paper discusses an ontology-based approach$\backslash$nto multi-layered conceptual spatial mapping that provides a common$\backslash$nground for human-robot dialogue. This is achieved by combining acquired$\backslash$nknowledge with innate conceptual commonsense knowledge in order to$\backslash$ninfer new knowledge. The architecture bridges the gap between the$\backslash$nrich semantic representations of the meaning expressed by verbal$\backslash$nutterances on the one hand and the robot's internal sensor-based$\backslash$nworld representation on the other. It is thus possible to establish$\backslash$nreferences to spatial areas in a situated dialogue between a human$\backslash$nand a robot about their environment. The resulting conceptual descriptions$\backslash$nrepresent qualitative knowledge about locations in the environment$\backslash$nthat can serve as a basis for achieving a notion of situational awareness.},
author = {Kruijff, Geert Jan M and Zender, Hendrik and Jensfelt, Patric and Christensen, Henrik I.},
doi = {10.5772/5701},
file = {:home/ckennington/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Kruijff et al. - 2007 - Situated dialogue and spatial organization What, where... and why(3).pdf:pdf},
issn = {17298806},
journal = {International Journal of Advanced Robotic Systems},
keywords = {Conceptual spatial mapping,Human-robot interaction,Situated dialogue},
number = {1 SPEC. ISS.},
pages = {125--138},
title = {{Situated dialogue and spatial organization: What, where... and why?}},
volume = {4},
year = {2007}
}
@article{Boleda2016,
abstract = {One of the most basic functions of language is to refer to objects in a shared scene. Modeling reference with continuous representations is challenging because it requires individuation, i.e., tracking and distinguishing an arbitrary number of referents. We introduce a neural network model that, given a definite description and a set of objects represented by natural images, points to the intended object if the expression has a unique referent, or indicates a failure, if it does not. The model, directly trained on reference acts, is competitive with a pipeline manually engineered to perform the same task, both when referents are purely visual, and when they are characterized by a combination of visual and linguistic properties.},
archivePrefix = {arXiv},
