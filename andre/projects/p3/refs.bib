}
@article{Rohrbach2010,
abstract = {Remarkable performance has been reported to recognize single object$\backslash$nclasses. Scalability to large numbers of classes however remains$\backslash$nan important challenge for today's recognition methods. Several authors$\backslash$nhave promoted knowledge transfer between classes as a key ingredient$\backslash$nto address this challenge. However, in previous work the decision$\backslash$nwhich knowledge to transfer has required either manual supervision$\backslash$nor at least a few training examples limiting the scalability of these$\backslash$napproaches. In this work we explicitly address the question of how$\backslash$nto automatically decide which information to transfer between classes$\backslash$nwithout the need of any human intervention. For this we tap into$\backslash$nlinguistic knowledge bases to provide the semantic link between sources$\backslash$n(what) and targets (where) of knowledge transfer. We provide a rigorous$\backslash$nexperimental evaluation of different knowledge bases and state-of-the-art$\backslash$ntechniques from Natural Language Processing which goes far beyond$\backslash$nthe limited use of language in related work. We also give insights$\backslash$ninto the applicability (why) of different knowledge sources and similarity$\backslash$nmeasures for knowledge transfer.},
author = {Rohrbach, Marcus and Stark, Michael and Szarvas, Gy{\"{o}}rgy and Gurevych, Iryna and Schiele, Bernt},
doi = {10.1109/CVPR.2010.5540121},
file = {:home/ckennington/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Rohrbach et al. - 2010 - What helps where - and why Semantic relatedness for knowledge transfer.pdf:pdf},
isbn = {9781424469840},
issn = {10636919},
journal = {Proceedings of the IEEE Computer Society Conference on Computer Vision and Pattern Recognition},
number = {c},
pages = {910--917},
pmid = {18573770},
title = {{What helps where - and why? Semantic relatedness for knowledge transfer}},
volume = {1},
year = {2010}
}
@article{Lampert2009,
abstract = {We study the problem of object classification when training and test classes are disjoint, i.e. no training examples of the target classes are available. This setup has hardly been studied in computer vision research, but it is the rule rather than the exception, because the world contains tens of thousands of different object classes and for only a very few of them image, collections have been formed and annotated with suitable class labels. In this paper, we tackle the problem by introducing attribute-based classification. It performs object detection based on a human-specified high-level description of the target objects instead of training images. The description consists of arbitrary semantic attributes, like shape, color or even geographic information. Because such properties transcend the specific learning task at hand, they can be pre-learned, e.g. from image datasets unrelated to the current task. Afterwards, new classes can be detected based on their attribute representation, without the need for a new training phase. In order to evaluate our method and to facilitate research in this area, we have assembled a new large-scale dataset, ldquoAnimals with Attributesrdquo, of over 30,000 animal images that match the 50 classes in Osherson's classic table of how strongly humans associate 85 semantic attributes with animal classes. Our experiments show that by using an attribute layer it is indeed possible to build a learning object detection system that does not require any training images of the target classes.},
author = {Lampert, Christoph H. and Nickisch, Hannes and Harmeling, Stefan},
doi = {10.1109/CVPRW.2009.5206594},
file = {:home/ckennington/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Lampert, Nickisch, Harmeling - 2009 - Learning to detect unseen object classes by between-class attribute transfer.pdf:pdf},
isbn = {9781424439935},
issn = {1063-6919},
journal = {2009 IEEE Computer Society Conference on Computer Vision and Pattern Recognition Workshops, CVPR Workshops 2009},
pages = {951--958},
title = {{Learning to detect unseen object classes by between-class attribute transfer}},
year = {2009}
}
@article{Chiu2016a,
abstract = {Named entity recognition is a challenging task that has traditionally required large amounts of knowledge in the form of feature engineering and lexicons to achieve high performance. In this paper, we present a novel neural network architecture that automatically detects word- and character-level features using a hybrid bidirectional LSTM and CNN architecture, eliminating the need for most feature engineering. We also propose a novel method of encoding partial lexicon matches in neural networks and compare it to existing exact match approaches. Extensive evaluation shows that, given only tokenized text, publicly available word vectors, and an automatically constructed lexicon from open sources, our system is able to surpass the reported state-of-the-art on the OntoNotes 5.0 dataset by 2.35 F1 points and achieves competitive results on the CoNLL 2003 dataset, rivaling systems that employ heavy feature engineering, proprietary lexicons, and rich entity linking information.},
