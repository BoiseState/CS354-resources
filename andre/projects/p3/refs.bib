issn = {15587916},
journal = {IEEE Transactions on Audio, Speech and Language Processing},
keywords = {Generative and discriminative models,nlu,semantics,spoken dialogue systems,system combination},
mendeley-tags = {nlu,semantics},
number = {6},
pages = {1569--1583},
title = {{Comparing stochastic approaches to spoken language understanding in multiple languages}},
volume = {19},
year = {2011}
}
