@article{Gupta2006,
abstract = {Spoken language understanding (SLU) aims at extracting meaning from natural language speech. Over the past decade, a variety of practical goal-oriented spoken dialog systems have been built for limited domains. SLU in these systems ranges from understanding predetermined phrases through fixed grammars, extracting some predefined named entities, extracting users' intents for call classification, to combinations of users' intents and named entities. In this paper, we present the SLU system of VoiceTone{\&}reg; (a service provided by AT{\&}T where AT{\&}T develops, deploys and hosts spoken dialog applications for enterprise customers). The SLU system includes extracting both intents and the named entities from the users' utterances. For intent determination, we use statistical classifiers trained from labeled data, and for named entity extraction we use rule-based fixed grammars. The focus of our work is to exploit data and to use machine learning techniques to create scalable SLU systems which can be quickly deployed for new domains with minimal human intervention. These objectives are achieved by 1) using the predicate-argument representation of semantic content of an utterance; 2) extending statistical classifiers to seamlessly integrate hand crafted classification rules with the rules learned from data; and 3) developing an active learning framework to minimize the human labeling effort for quickly building the classifier models and adapting them to changes. We present an evaluation of this system using two deployed applications of VoiceTone{\&}reg;.},
author = {Gupta, Narendra and Tur, Gokhan},
doi = {10.1109/TSA.2005.854085},
file = {:home/ckennington/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Gupta, Tur - 2006 - The AT{\&}T spoken language understanding system(2).pdf:pdf},
issn = {1558-7916},
journal = {{\ldots} , and Language {\ldots}},
keywords = {Named entities,semantic classification,semantic representation,toolkits},
mendeley-tags = {toolkits},
number = {1},
pages = {213--222},
title = {{The AT{\&}T spoken language understanding system}},
url = {http://ieeexplore.ieee.org/xpls/abs{\_}all.jsp?arnumber=1561278},
volume = {14},
year = {2006}
}
@inproceedings{Juang2000,
abstract = {The promise of a powerful computing device to help people in$\backslash$nproductivity as well as in recreation can only be realized with proper$\backslash$nhuman-machine communication. Automatic recognition and understanding of$\backslash$nspoken language is the first step toward natural human-machine$\backslash$ninteraction. Research in this field has produced remarkable results,$\backslash$nleading to many exciting expectations and new challenges. We summarize$\backslash$nthe development of the spoken language technology from both a vertical$\backslash$n(chronology) and a horizontal (spectrum of technical approaches)$\backslash$nperspective. We highlight the introduction of statistical methods in$\backslash$ndealing with language-related problems, as this represents a paradigm$\backslash$nshift in the research field of spoken language processing. Statistical$\backslash$nmethods are designed to allow the machine to learn structural$\backslash$nregularities in the speech signal, directly from data, for the purpose$\backslash$nof automatic speech recognition and understanding. Research results in$\backslash$nspoken language processing have led to a number of successful$\backslash$napplications, ranging from dictation software for personal computers and$\backslash$ntelephone-call processing systems for automatic call routing, to$\backslash$nautomatic sub-captioning for television broadcasts. We analyze the$\backslash$ntechnical successes that support these applications. Along with an$\backslash$nassessment of the state of the art in this broad technical field, we$\backslash$nalso discuss the limitations of the current technology, and point out$\backslash$nthe challenges that are ahead. This paper presents an accurate overview$\backslash$nof spoken language technology as a basis to inspire future$\backslash$nadvances},
