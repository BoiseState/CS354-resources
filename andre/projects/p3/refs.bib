journal = {Computational Linguistics},
keywords = {nlu,syntax},
mendeley-tags = {nlu,syntax},
number = {4},
pages = {513--553},
publisher = {MIT Press},
title = {{Algorithms for Deterministic Incremental Dependency Parsing}},
url = {http://www.mitpressjournals.org/doi/abs/10.1162/coli.07-056-R1-07-027},
volume = {34},
year = {2008}
