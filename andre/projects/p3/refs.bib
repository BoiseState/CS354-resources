@article{glenberg2002grounding,
abstract = {We report a new phenomenon associated with language comprehension: the action-sentence compatibility effect (ACE). Participants judged whether sentences were sensible by making a response that required moving toward or away from their bodies. When a sentence implied action in one direction (e.g., "Close the drawer" implies action away from the body), the participants had difficulty making a sensibility judgment requiring a response in the opposite direction. The ACE was demonstrated for three sentences types: imperative sentences, sentences describing the transfer of concrete objects, and sentences describing the transfer of abstract entities, such as "Liz told you the story." These dataare inconsistent with theories of language comprehension in which meaning is represented as a set of relations among nodes. Instead, the data support an embodied theory of meaning that relates the meaning of sentences to human action.},
author = {Rohlfing, Katharina J. and Tani, Jun},
doi = {10.1109/TAMD.2011.2140890},
file = {:home/ckennington/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Rohlfing, Tani - 2011 - Grounding language in action(2).pdf:pdf},
