author = {Chiu, Jason P. C. and Nichols, Eric},
eprint = {1511.08308},
file = {:home/ckennington/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Chiu, Nichols - 2016 - Named Entity Recognition with Bidirectional LSTM-CNNs.pdf:pdf},
